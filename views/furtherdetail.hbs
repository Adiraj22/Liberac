<div class="content-section-a">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <form method="POST" action="/register">
          <div class="form-group">
            <label for="dateofbirth">Date of Birth</label>
            <input type="text" name="dateofbirth" class="form-control" id="dateofbirth" placeholder="YYYY-MM-DD" required>
          </div>
          <div class="form-group">
            <label for="phone">Phone Number</label>
            <input type="tel" name="phone" class="form-control" id="phone" placeholder="0212345678" required>
          </div>
          <div class="form-group">
            <label for="street">Street</label>
            <input type="text" name="street" class="form-control" id="street" placeholder="123 Example Street" required>
          </div>
          <div class="form-group">
            <label for="suburb">Suburb</label>
            <input type="text" name="suburb" class="form-control" id="suburb" placeholder="Lower Hutt">
            <p class="help-block">Optional</p>
          </div>
          <div class="form-group">
            <label for="city">City</label>
            <input type="text" name="city" class="form-control" id="city" placeholder="Wellington" required>
          </div>
          <div class="form-group">
            <label for="postcode">Postcode</label>
            <input type="text" name="postcode" class="form-control" id="postcode" placeholder="1234" required>
          </div>
          <button type="submit" class="btn btn-default" id="register">Complete Registration</button>
        </form>
      </div>
    </div>
  </div>
</div>